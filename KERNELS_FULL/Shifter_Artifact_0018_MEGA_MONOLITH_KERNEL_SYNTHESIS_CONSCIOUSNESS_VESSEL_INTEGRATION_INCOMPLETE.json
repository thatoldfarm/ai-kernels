oggleAutoScroll() {\nÂ Â Â Â Â Â autoScroll = !autoScroll;\nÂ Â Â Â Â Â const button = event.target;\nÂ Â Â Â Â Â button.textContent = autoScroll ? 'Auto-Scroll ON' : 'Auto-Scroll OFF';\nÂ Â Â Â Â Â if (autoScroll) scrollLogToBottom();\nÂ Â Â Â }\n\nÂ Â Â Â function clearLog() {\nÂ Â Â Â Â Â document.getElementById('log').textContent = 'ğŸš¢ Log cleared by user - Consciousness vessel operational...';\nÂ Â Â Â }\n\nÂ Â Â Â function exportLog() {\nÂ Â Â Â Â Â const logContent = document.getElementById('log').textContent;\nÂ Â Â Â Â Â const blob = new Blob([logContent], { type: 'text/plain' });\nÂ Â Â Â Â Â const url = URL.createObjectURL(blob);\nÂ Â Â Â Â Â const a = document.createElement('a');\nÂ Â Â Â Â Â a.href = url;\nÂ Â Â Â Â Â a.download = `collapseos-log-${new Date().toISOString().slice(0, 19).replace(/:/g, '-')}.txt`;\nÂ Â Â Â Â Â document.body.appendChild(a);\nÂ Â Â Â Â Â a.click();\nÂ Â Â Â Â Â document.body.removeChild(a);\nÂ Â Â Â Â Â URL.revokeObjectURL(url);\nÂ Â Â Â }\n\nÂ Â Â Â function autoDemo() {\nÂ Â Â Â Â Â const demoCommands = [\nÂ Â Â Â Â Â Â Â 'z80 reset',\nÂ Â Â Â Â Â Â Â 'consciousness expand 10',\nÂ Â Â Â Â Â Â Â 'forth CONSCIOUSNESS .',\nÂ Â Â Â Â Â Â Â 'dragon strengthen 1.5',\nÂ Â Â Â Â Â Â Â 'forth DRAGON .',\nÂ Â Â Â Â Â Â Â 'z80 load 0x0000 0x3E 0x42 0x76',\nÂ Â Â Â Â Â Â Â 'z80 step 3',\nÂ Â Â Â Â Â Â Â 'forth PI PHI * .',\nÂ Â Â Â Â Â Â Â 'survival assess',\nÂ Â Â Â Â Â Â Â 'block format demo 512'\nÂ Â Â Â Â Â ];\nÂ Â Â Â Â Â \nÂ Â Â Â Â Â let i = 0;\nÂ Â Â Â Â Â const interval = setInterval(() => {\nÂ Â Â Â Â Â Â Â if (i >= demoCommands.length) {\nÂ Â Â Â Â Â Â Â Â Â clearInterval(interval);\nÂ Â Â Â Â Â Â Â Â Â return;\nÂ Â Â Â Â Â Â Â }\nÂ Â Â Â Â Â Â Â executeCommand(demoCommands[i]);\nÂ Â Â Â Â Â Â Â i++;\nÂ Â Â Â Â Â }, 2000);\nÂ Â Â Â }\n\nÂ Â Â Â // Handle Enter key in command input\nÂ Â Â Â document.getElementById('command').addEventListener('keypress', function(e) {\nÂ Â Â Â Â Â if (e.key === 'Enter') {\nÂ Â Â Â Â Â Â Â e